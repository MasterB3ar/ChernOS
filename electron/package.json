{
  "name": "chernos-desktop",
  "version": "1.3.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack:win": "electron-packager . ChernOS --platform=win32 --arch=x64 --out=dist --overwrite",
    "pack:linux": "electron-packager . ChernOS --platform=linux --arch=x64 --out=dist --overwrite",
    "pack:mac": "electron-packager . ChernOS --platform=darwin --arch=arm64 --out=dist --overwrite"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-packager": "^17.1.2"
  }
}
